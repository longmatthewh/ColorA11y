{
  "name": "colora11y",
  "version": "1.1.5",
  "repository": {
    "type": "git",
    "url": "https://github.com/longmatthewh/ColorA11y.git"
  },
  "license": "MIT",
  "keywords": [
    "accessibility",
    "a11y",
    "color contrast",
    "WCAG"
  ],
  "author": "Matt Long <longmatthewh@gmail.com>",
  "main": "./colora11y.js",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.18.1",
    "eslint-config-google": "^0.9.1",
    "jasmine": "^3.0.0",
    "uglify-js": "^3.3.11"
  },
  "scripts": {
    "eslint": "eslint colora11y.js",
    "babel": "babel colora11y.js --out-file colora11y.min.js",
    "uglify": "uglifyjs colora11y.min.js -o colora11y.min.js -c -m",
    "build": "npm run babel && npm run uglify",
    "test": "npm run eslint && jasmine"
